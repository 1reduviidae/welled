
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'virtual/mac80211_hwsim/hwsim0'
	option txpower '20'
	option channel '11'
	option hwmode '11g'
	option country '00'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option encryption 'psk2'
	option key 'tartans@1'
	option macaddr '00:0c:41:00:00:00'
	option ssid 'OpenWrt'
	option network 'wlan'
